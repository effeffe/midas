COMMENT "Sequencer Test File"

RUNDESCRIPTION "Sequencer Test"

PARAM num_runs,Number of runs

CALL init
LOOP 3
  TRANSITION start
  WAIT seconds, 10
  TRANSITION stop
  WAIT seconds, 3
ENDLOOP

SUBROUTINE init
  MESSAGE "Initialization started"
  LOOP 4
    WAIT seconds, 3
  ENDLOOP
  MESSAGE "Initialization completed", 1
ENDSUBROUTINE
